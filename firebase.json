{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      }, {
        "source": "/schedule",
        "destination": "/schedule.html"
      }, {
        "source": "/earnings",
        "destination": "/earnings.html"
      }, {
        "source": "/dashboard",
        "destination": "/dashboard.html"
      }
    ]
  }
}
